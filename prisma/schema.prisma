generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Locale {
  EN
  PL
}

model Guild {
  id     String  @id
  locale Locale
  owner  String?

  updatedAt DateTime @default(now()) @updatedAt
  createdAt DateTime @default(now())
}
